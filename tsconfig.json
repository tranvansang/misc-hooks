{
	"compilerOptions": {
		"strict": true,
		"target": "ESNext",
		"module": "ESNext",
		"baseUrl": ".",
		"outDir": ".",
		"declaration": true,
		"inlineSourceMap": true,
		"downlevelIteration": true,
		"strictNullChecks": true,
		"moduleResolution": "Node",
		"esModuleInterop": true
	},
	"include": ["index.ts", "atom.ts", "disposer.ts"],
	"exclude": ["node_modules"]
}
